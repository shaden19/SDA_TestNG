<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="NegativeLoginTests">
    <test name="correct user name and wrong password">
        <classes>
            <class name="sda.tests.day16.Task03">
                <parameter name="username" value="student" />
                <parameter name="password" value="WrongPassword" />
            </class>
        </classes>
    </test>
    <test name="wrong user name and correct password">
        <classes>
            <class name="sda.tests.day16.Task03">
                <parameter name="username" value="WrongUsername" />
                <parameter name="password" value="Password123" />
            </class>
        </classes>
    </test>
    <test name="wrong user name and wrong password">
        <classes>
            <class name="sda.tests.day16.Task03">
                <parameter name="username" value="WrongUsername" />
                <parameter name="password" value="WrongPassword" />
            </class>
        </classes>
    </test>
    <test name="empty user name correct password">
        <classes>
            <class name="sda.tests.day16.Task03">
                <parameter name="username" value="" />
                <parameter name="password" value="Password123" />
            </class>
        </classes>
    </test>
</suite>














